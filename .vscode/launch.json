        {
            "name": "upserver",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "env": {"GOMAXPROCS":1},
            "program": "${workspaceFolder}/cmd/uploadserver",
            //"args" : ["-adduser","zahar2"]
            // "args": ["-log","${workspaceFolder}/cmd/uploadserver/testdata/storageroot/../service.log","-root",
            //     "${workspaceFolder}/cmd/uploadserver/testdata/storageroot",
            //     "-listenOn","192.168.3.53:64002",
            // ]
            "args": ["-log","${workspaceFolder}/cmd/uploadserver/testdata/storageroot/../service.log","-root",
                "j:/b",
                "-listenOn","192.168.3.53:64002",
            ]


        },
        // runs uploader
        {
            
            "name": "uploaderHTTP",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {} ,
             "args": [
                //"-file","${workspaceFolder}/cmd/uploader/testdata/testbackups/from_8_4_1c77_BasesDirs_2020-01.rar",
                "-file","${workspaceFolder}/cmd/uploader/testdata/testbackups/sendfile.rar",
                "-username","zahar",
                "-passwordfile","./logins.json",
                "-service", "https://192.168.3.53:64000/upload"
            
             ]
        },
        {
            
            "name": "uploaderFTP",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "env": {} ,
             "args": [
                "-file","${workspaceFolder}/cmd/uploader/testdata/testbackups/sendfile.rar",
                "-username","zahar",
                "-passwordfile","./logins.json",
                "-service", "ftp://192.168.1.106:2121"
            
             ]
        },
        {
            
            "name": "uploaderSavePass",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${fileDirname}",
            "buildFlags": "-x -ldflags='-X github.com/zavla/upload/logins.PasswordForTest=pass'",
            "env": {} ,
             "args": [
                "-username","zahar",
                "-passwordfile","./logins.json",
                "-savepassword", "true"
            
             ]
        },

        {
            "name": "Attach to Srv",
